$(window).scroll(function() {
    if ($(this).scrollTop() > 1) {
        $('.header').addClass("sticky");
        $('.latest-notice-slider').addClass("stickyBottom").slick('slickGoTo', 0);
        $('.allNotification').addClass("moveUp");
    } else {
        $('header').removeClass("sticky");
        $('.latest-notice-slider').removeClass("stickyBottom");
        $('.allNotification').removeClass("moveUp");
    }
    if ($(window).scrollTop() >= 500) {
        $('.gotoTop').addClass('moveUp');
    } else {
        $('.gotoTop').removeClass('moveUp');
    }
});


$('.gotoTop__icon').click(function() {
    $("html, body").animate({ scrollTop: '0' }, 600);
});


document.addEventListener("DOMContentLoaded", function(){

window.addEventListener('scroll', function() {
   
    if (window.scrollY > 150) {
        document.getElementById('navTopMenus').classList.add('fixed-top');
        //add padding top to show content behind navbar Top
        navbar_height = document.querySelector('.navbar').offsetHeight;
        document.body.style.paddingTop = navbar_height + 'px';
    } else {
        document.getElementById('navTopMenus').classList.remove('fixed-top');
         // remove padding top from body
        document.body.style.paddingTop = '0';
    } 
});
}); 


